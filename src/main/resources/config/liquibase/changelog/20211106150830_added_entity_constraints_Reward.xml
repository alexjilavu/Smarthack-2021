<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.5.xsd">
    <!--
        Added the constraints for entity Reward.
    -->
    <changeSet id="20211106150830-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="icon_id"
                                 baseTableName="reward"
                                 constraintName="fk_reward__icon_id"
                                 referencedColumnNames="id"
                                 referencedTableName="icon"/>

        <addForeignKeyConstraint baseColumnNames="company_id"
                                 baseTableName="reward"
                                 constraintName="fk_reward__company_id"
                                 referencedColumnNames="id"
                                 referencedTableName="company"/>
    </changeSet>
</databaseChangeLog>
